{
  "name": "dossierfacile",
  "private": true,
  "license": "MIT",
  "version": "3.0.0",
  "description": "Frontend for DossierFacile",
  "repository": {
    "type": "git",
    "url": "git@github.com:MTES-MCT/Dossier-Facile-Frontend.git"
  },
  "workspaces": [
    "main",
    "www",
    "owner",
    "tenant",
    "tenantv3",
    "watermark",
    "df-shared",
    "df-shared-next",
    "e2e-tests",
    "sb"
  ],
  "scripts": {
    "main": "yarn workspace main",
    "www": "yarn workspace www",
    "dfowner": "yarn workspace owner",
    "tenant": "yarn workspace tenant",
    "tenantv3": "yarn workspace tenantv3",
    "watermark": "yarn workspace watermark",
    "e2e": "yarn workspace e2e-tests",
    "sb": "yarn workspace sb",
    "update-lockfiles": "yarn && echo main www tenant tenantv3 owner watermark e2e-tests sb | xargs -n 1 cp yarn.lock"
  },
  "devDependencies": {
    "@storybook/vue-webpack5": "7.0.2"
  },
  "resolutions": {
    "trim": "^1.0.1",
    "trim-newlines": "^3.0.1",
    "dot-object": "^2.1.3",
    "glob-parent": "^5.1.2",
    "ejs": "3.1.7",
    "http-cache-semantics": "4.1.1",
    "@vee-validate/rules": "4.7.4",
    "minimatch": "^6.1.6",
    "semver": "7.5.2"
  }
}
