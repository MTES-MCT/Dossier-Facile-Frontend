{
  "name": "watermark",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/connect-history-api-fallback/**",
      "**/express/**"
    ]
  },
  "version": "0.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
