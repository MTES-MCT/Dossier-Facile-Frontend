{
  "name": "watermark",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/connect-history-api-fallback/**",
      "**/connect-history-api-fallback",
      "**/express/**",
      "**/express"
    ]
  },
  "version": "0.0.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "node server.js"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.10.1",
    "@sentry/tracing": "^7.51.2",
    "@sentry/vue": "^7.51.2",
    "@vee-validate/rules": "^4.5.11",
    "@vue/cli-plugin-babel": "5.0.8",
    "axios": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "df-shared-next": "0.1.0",
    "express": "^4.20.0",
    "vee-validate": "^4.11.3",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.17",
    "vue-router": "4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.54.0",
    "typescript": "5.0.2",
    "vite": "5.2.14",
    "vite-plugin-pwa": "^0.16.5",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "state"
        ]
      }
    ]
  }
}
