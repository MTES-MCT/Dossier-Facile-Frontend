{
  "name": "watermark",
  "private": true,
  "type": "module",
  "workspaces": {
    "nohoist": [
      "**/connect-history-api-fallback/**",
      "**/connect-history-api-fallback",
      "**/express/**",
      "**/express"
    ]
  },
  "version": "0.0.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "start": "node server.js"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.11.2",
    "@sentry/tracing": "7.119.2",
    "@sentry/vue": "7.119.2",
    "@vee-validate/rules": "^4.5.11",
    "axios": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "df-shared-next": "0.1.0",
    "express": "4.21.1",
    "vee-validate": "^4.11.3",
    "vue": "3.5.12",
    "vue-i18n": "^9.3.0-beta.17",
    "vue-router": "4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@vitejs/plugin-vue": "5.1.4",
    "sass": "1.79.4",
    "typescript": "5.6.2",
    "vite": "5.4.9",
    "vite-plugin-pwa": "0.20.5",
    "vue-tsc": "2.1.6"
  },
  "eslintConfig": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "state"
        ]
      }
    ]
  }
}
