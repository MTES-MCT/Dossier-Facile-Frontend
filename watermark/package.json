  "dependencies": {
    "@gouvfr/dsfr": "^1.6.0",
    "@vee-validate/rules": "^4.5.11",
    "@vue/cli-plugin-babel": "4.5.15",
    "axios": "^0.27.2",
    "connect-history-api-fallback": "^2.0.0",
    "df-shared-next": "0.1.0",
    "express": "^4.17.3",
    "vee-validate": "^4.6.2",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "sass": "^1.54.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue-tsc": "^0.38.4"
  },
  "eslintConfig": {
    "no-param-reassign": [
