{
  "name": "watermark",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/connect-history-api-fallback/**",
      "**/connect-history-api-fallback",
      "**/express/**",
      "**/express"
    ]
  },
  "version": "0.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "node server.js"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.9.0",
    "@vee-validate/rules": "^4.5.11",
    "@vue/cli-plugin-babel": "5.0.8",
    "axios": "^1.3.4",
    "connect-history-api-fallback": "^2.0.0",
    "df-shared-next": "0.1.0",
    "express": "^4.18.1",
    "vee-validate": "^4.6.2",
    "vue": "^3.2.37",
    "vue-i18n": "^9.3.0-beta.17",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.54.0",
    "typescript": "5.0.2",
    "vite": "^4.2.3",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "state"
        ]
      }
    ]
  }
}
