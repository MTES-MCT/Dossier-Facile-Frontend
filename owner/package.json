{
  "name": "owner",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "test:unit": "vitest",
    "fbuild": "vue-tsc --noEmit && vite build",
    "tsc": "vue-tsc --noEmit -p tsconfig.app.json",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@gouvfr/dsfr": "catalog:",
    "@remixicon/vue": "4.6.0",
    "@sentry/vite-plugin": "catalog:",
    "@sentry/vue": "catalog:",
    "@types/js-cookie": "3.0.6",
    "axios": "catalog:",
    "date-fns": "4.1.0",
    "df-shared-next": "workspace:*",
    "js-cookie": "3.0.5",
    "keycloak-js": "26.2.0",
    "pinia": "catalog:",
    "vee-validate": "catalog:",
    "vue": "catalog:",
    "vue-i18n": "catalog:",
    "vue-router": "catalog:",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "catalog:",
    "@tsconfig/node18": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@vue/eslint-config-prettier": "catalog:",
    "@vue/eslint-config-typescript": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "eslintConfig": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "state"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.26.0"
}